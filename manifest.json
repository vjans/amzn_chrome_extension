{
	"name": "Amazon updater",
	"description":"tells you if there are reduced products in your wishlist",
	"version":"0.0.1",
	"manifest_version": 3,
	"icons":{
		"16": "./images/icon1.png",
		"32": "./images/icon2.png",
		"48": "./images/icon3.png",
		"128": "./images/icon4.png"
	},
	"background":{
		"service_worker":"background.js"
	},
	"action":{
		"default_popup":"./popup.html",
		"default_icons":{

		"16": "./images/icon1.png",
		"32": "./images/icon2.png",
		"48": "./images/icon3.png",
		"128": "./images/icon4.png"
		}
	},
	"options_page": "./options.html",
	"permissions":[
		"activeTab",
		"tabs",
		"storage",
		"scripting"
	],
	"host_permissions":[
		"https://*.amazon.com/"
	]

}
